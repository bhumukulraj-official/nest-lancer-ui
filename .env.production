# Production Environment Variables
# NOTE: Only client-safe variables (prefixed with VITE_) are exposed to frontend

# Backend API Configuration (Production)
VITE_API_BASE_URL=https://api.nestlancer.com/api
VITE_WS_URL=wss://api.nestlancer.com

# Third-party Integration (Production Keys)
VITE_RAZORPAY_KEY_ID=rzp_live_production_key
VITE_CLOUDINARY_CLOUD_NAME=nestlancer-prod
VITE_CLOUDINARY_UPLOAD_PRESET=prod_upload_preset

# Analytics & Monitoring (Production)
VITE_GA_TRACKING_ID=G-PROD12345678

# Production Features
VITE_ENABLE_DEV_TOOLS=false
VITE_PWA_ENABLED=true
VITE_MAINTENANCE_MODE=false

# Internationalization
VITE_DEFAULT_LOCALE=en
VITE_FALLBACK_LOCALE=en

# Performance & Debugging (Production)
VITE_LOG_LEVEL=error
VITE_API_TIMEOUT=15000

# App Metadata
VITE_APP_NAME=NestLancer
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Professional freelancing platform for project management and collaboration
