{
  "name": "nest-lancer-frontend",
  "private": true,
  "version": "1.0.0",
  "description": "UI-only freelancing platform frontend built with React 18, TypeScript, and Material-UI",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "analyze": "ANALYZE=true npm run build",
    "analyze:serve": "ANALYZE=true npm run build && npx serve dist",
    "test:suite": "bash scripts/test-suite.sh",
    "test:suite:all": "bash scripts/test-suite.sh --all",
    "prepare": "husky install"
  },
  "dependencies": {
    "@cloudinary/react": "^1.3.0",
    "@cloudinary/url-gen": "^1.10.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.0",
    "@mui/icons-material": "^5.14.0",
    "@mui/lab": "^5.0.0-alpha.145",
    "@mui/material": "^5.14.0",
    "@mui/x-data-grid": "^6.15.0",
    "@mui/x-date-pickers": "^6.15.0",
    "@tanstack/react-query": "^4.32.0",
    "@tanstack/react-query-devtools": "^4.32.0",
    "axios": "^1.5.0",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.30.0",
    "dompurify": "^3.0.0",
    "framer-motion": "^10.16.0",
    "i18next": "^23.4.0",
    "i18next-browser-languagedetector": "^7.1.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "razorpay": "^2.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.45.0",
    "react-hot-toast": "^2.4.0",
    "react-i18next": "^13.2.0",
    "react-responsive": "^9.0.0",
    "react-router-dom": "^6.15.0",
    "recharts": "^2.7.0",
    "socket.io-client": "^4.7.0",
    "web-vitals": "^3.4.0",
    "workbox-window": "^7.0.0",
    "yup": "^1.2.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/crypto-js": "^4.1.1",
    "@types/dompurify": "^3.0.2",
    "@types/lodash": "^4.14.196",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/ui": "^0.34.0",
    "eslint": "^8.47.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "msw": "^1.3.0",
    "prettier": "^3.0.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "storybook": "^8.6.14",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-pwa": "^0.16.4",
    "vitest": "^0.34.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
